    "content": "\r\n\r\n\r\n<div class=\"hiq-page-wrap\">\r\n<div class=\"hiq-t-wrap\">\r\n\t\t\r\n  \t  <div class=\"hiq-t \">\r\n\t\t\t\t<ul class=\"breadcrumbs\"><li><a hqid=\"1906713\" href=\"/home\" class=\"breadcrumb-links\">ON24 REST API</a></li><li><span>&rsaquo;&rsaquo;</span><a hqid=\"1906768\" href=\"/analytics\" class=\"breadcrumb-links\">Analytics</a></li><li><span>&rsaquo;&rsaquo;</span><a hqid=\"1906770\" href=\"/event-level\" class=\"breadcrumb-links\">Event Level</a></li><li> <span>\u203a\u203a</span>Registrants</li></ul>  \t\r\n\t\t<h3><strong><span style=\"font-size:24px;\"><span style=\"font-family:Arial,Helvetica,sans-serif;\">/v2/client/{clientId}/event/{eventId}/registrant</span></span></strong></h3>\r\n\r\n<h3><strong><span style=\"font-size:24px;\"><span style=\"font-family:Arial,Helvetica,sans-serif;\">METHOD: GET</span></span></strong></h3>\r\n\r\n<p><span style=\"font-size:14px;\"><span style=\"font-family:Arial,Helvetica,sans-serif;\">Retrieves a list of registrants for a particular event.</span></span></p>\r\n\r\n<p><strong><span style=\"font-size:14px;\"><span style=\"font-family:Arial,Helvetica,sans-serif;\">Parameters:</span></span></strong></p>\r\n\r\n<table class=\"table table-striped table-bordered\">\r\n\t<thead>\r\n\t\t<tr>\r\n\t\t\t<th><span style=\"font-size:14px;\"><span style=\"font-family:Arial,Helvetica,sans-serif;\">Name</span></span></th>\r\n\t\t\t<th><span style=\"font-size:14px;\"><span style=\"font-family:Arial,Helvetica,sans-serif;\">Located In</span></span></th>\r\n\t\t\t<th><span style=\"font-size:14px;\"><span style=\"font-family:Arial,Helvetica,sans-serif;\">Required</span></span></th>\r\n\t\t\t<th><span style=\"font-size:14px;\"><span style=\"font-family:Arial,Helvetica,sans-serif;\">Description</span></span></th>\r\n\t\t\t<th><span style=\"font-size:14px;\"><span style=\"font-family:Arial,Helvetica,sans-serif;\">Default</span></span></th>\r\n\t\t\t<th><span style=\"font-size:14px;\"><span style=\"font-family:Arial,Helvetica,sans-serif;\">Schema</span></span></th>\r\n\t\t\t<th><span style=\"font-size:14px;\"><span style=\"font-family:Arial,Helvetica,sans-serif;\">Example</span></span></th>\r\n\t\t</tr>\r\n\t</thead>\r\n\t<tbody>\r\n\t\t<tr>\r\n\t\t\t<td><span style=\"font-size:14px;\"><span style=\"font-family:Arial,Helvetica,sans-serif;\"><strong>clientId</strong></span></span></td>\r\n\t\t\t<td><span style=\"font-size:14px;\"><span style=\"font-family:Arial,Helvetica,sans-serif;\">path</span></span></td>\r\n\t\t\t<td><span style=\"font-size:14px;\"><span style=\"font-family:Arial,Helvetica,sans-serif;\">yes</span></span></td>\r\n\t\t\t<td><span style=\"font-size:14px;\"><span style=\"font-family:Arial,Helvetica,sans-serif;\">The client id</span></span></td>\r\n\t\t\t<td><span style=\"font-size:14px;\"><span style=\"font-family:Arial,Helvetica,sans-serif;\">-</span></span></td>\r\n\t\t\t<td><span style=\"font-size:14px;\"><span style=\"font-family:Arial,Helvetica,sans-serif;\">number</span></span></td>\r\n\t\t\t<td><span style=\"font-size:14px;\"><span style=\"font-family:Arial,Helvetica,sans-serif;\">1498</span></span></td>\r\n\t\t</tr>\r\n\t\t<tr>\r\n\t\t\t<td><span style=\"font-size:14px;\"><span style=\"font-family:Arial,Helvetica,sans-serif;\"><strong>eventId</strong></span></span></td>\r\n\t\t\t<td><span style=\"font-size:14px;\"><span style=\"font-family:Arial,Helvetica,sans-serif;\">path</span></span></td>\r\n\t\t\t<td><span style=\"font-size:14px;\"><span style=\"font-family:Arial,Helvetica,sans-serif;\">yes</span></span></td>\r\n\t\t\t<td><span style=\"font-size:14px;\"><span style=\"font-family:Arial,Helvetica,sans-serif;\">The event id</span></span></td>\r\n\t\t\t<td><span style=\"font-size:14px;\"><span style=\"font-family:Arial,Helvetica,sans-serif;\">-</span></span></td>\r\n\t\t\t<td><span style=\"font-size:14px;\"><span style=\"font-family:Arial,Helvetica,sans-serif;\">number</span></span></td>\r\n\t\t\t<td><span style=\"font-size:14px;\"><span style=\"fon